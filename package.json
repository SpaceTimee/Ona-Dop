    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.17.0",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^3.6.0",
